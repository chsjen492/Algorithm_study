#include<iostream>
#include <vector>

using namespace std;
int ret[5]={0,};

void factorization(int& a, int b, int n){
    int cnt=0;
    while(1){
        if(a%b==0){
            a/=b;
            cnt++;
        }
        else
            ret[n]=cnt;
    }}
int main(int argc, char** argv)
{
	int test_case;
	int T;
    int n;

	cin>>T;
    vector <int> v(T);
    
	for(int i=0;i<T;i++){
        cin>>n;
        v.push_back(n);
    }
    
	for(test_case = 1; test_case <= T; ++test_case)
	{
		factorization(v[test_case-1], 2,0);
        factorization(v[test_case-1], 3,1);
        factorization(v[test_case-1], 5,2);
        factorization(v[test_case-1], 7,3);
        factorization(v[test_case-1], 11,4);
        
        cout<<"#"<<test_case<<" "<<ret[0]<<" "<<ret[1]<<" "<<ret[2]<<" "<<ret[3]<<" "<<ret[4]<<endl;
        
		
	}
	return 0;//정상종료시 반드시 0을 리턴해야합니다.
}
